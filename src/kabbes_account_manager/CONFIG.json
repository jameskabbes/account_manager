{
    "working":{
        "!Dir": "{{cwd.Dir}}"
    },
    "remote":{
        "credentials":{
            "!Dir":{
                "$ref": "{{working.Dir}}",
                "method": {
                    "name": "join_Dir",
                    "kwargs":{
                        "path": "remote_credentials"
                    }    
                }
            }
        },
        "accounts":{
            "!Dir":{
                "$ref": "{{remote.credentials.Dir}}",
                "method": {
                    "name": "join_Dir",
                    "kwargs":{
                        "path": "Accounts"
                    }
                }
            }
        }
    },
    "local":{
        "credentials":{
            "!Dir":{
                "$ref": "{{working.Dir}}",
                "method": {
                    "name": "join_Dir",
                    "kwargs":{
                        "path": "local_credentials"
                    }    
                }
            }
        },
        "accounts":{
            "!Dir":{
                "$ref": "{{local.credentials.Dir}}",
                "method": {
                    "name": "join_Dir",
                    "kwargs":{
                        "path": "Data/accounts_backup"
                    }
                }
            }
        }
    },

    "keys":{
        "private":{
            "!Path":{
                "$ref": "{{local.credentials.Dir}}",
                "method": {
                    "name": "join_Path",
                    "kwargs":{
                        "path": "Data/privatekey"
                    }
                }
            }
        },
        "public":{
            "!Path":{
                "$ref": "{{remote.credentials.Dir}}",
                "method": {
                    "name": "join_Path",
                    "kwargs":{
                        "path": "publickey"
                    }
                }
            }
        }
    },

    "file_input":{
        "!Path":{
            "$ref": "{{working.Dir}}",
            "method": {
                "name": "join_Path",
                "kwargs":{
                    "path": "file_input.txt"
                }
            }
        }
    },
    "decrypted_export":{
        "!Path":{
            "$ref": "{{working.Dir}}",
            "method": {
                "name": "join_Path",
                "kwargs":{
                    "path": "decrypted_export.txt"
                }
            }
        }
    },
    "backup":{
        "!Dir":{
            "$ref": "{{Dir}}",
            "!new_path":{
                "$ref": "{{Dir}}",
                "method": {
                    "name": "lowest"
                }
            },
            "method": {
                "name": "join_Dir",
                "kwargs":{
                    "path": "{{backup.!Dir.new_path}}"
                }
            }
        }
    },

    "database_name": null
}